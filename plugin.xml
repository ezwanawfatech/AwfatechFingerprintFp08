<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-awfatechfingerprint" version="1.0.0"
    xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <name>AwfatechFingerprintFp08</name>

    <js-module name="AwfatechFingerprintFp08" src="www/AwfatechFingerprintFp08.js">
        <clobbers target="cordova.plugins.AwfatechFingerprintFp08" />
    </js-module>

    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="AwfatechFingerprintFp08">
                <param name="android-package" value="cordova.plugin.awfatechfingerprint.AwfatechFingerprintFp08" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/AwfatechFingerprintFp08.java" target-dir="src/cordova/plugin/awfatechfingerprint/AwfatechFingerprintFp08" />
    </platform>
    
</plugin>